{
  "name": "rinawarp-ai-cloud",
  "version": "2.0.0",
  "description": "RinaWarp AI - Cloud-based AI assistant service with terminal integration",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run lint && npm test",
    "test": "jest",
    "lint": "eslint src/",
    "docker:build": "docker build -t rinawarp-ai .",
    "docker:run": "docker run -p 3000:3000 --env-file .env rinawarp-ai",
    "deploy": "npm run build && npm run docker:build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@google-cloud/aiplatform": "^3.0.0",
    "@langchain/community": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "@langchain/openai": "^0.1.0",
    "axios": "^1.6.0",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.0",
    "node-cache": "^5.1.2",
    "openai": "^4.47.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  }
}
