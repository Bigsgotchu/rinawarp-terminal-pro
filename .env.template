# Code Signing Environment Variables Template
# Copy this file to .env and fill in the actual values
# NEVER commit real credentials to version control!

# Linux GPG Signing
GPG_KEY_ID={{GPG_KEY_ID}}
GPG_PASSPHRASE={{GPG_PASSPHRASE}}

# Windows Code Signing
WINDOWS_CERT_PATH={{WINDOWS_CERT_PATH}}
WINDOWS_CERT_PASSWORD={{WINDOWS_CERT_PASSWORD}}

# macOS Code Signing
APPLE_DEVELOPER_TEAM_ID={{APPLE_DEVELOPER_TEAM_ID}}
APPLE_CERT_NAME={{APPLE_CERT_NAME}}
APPLE_CERT_PASSWORD={{APPLE_CERT_PASSWORD}}

# App Store Connect (for macOS App Store distribution)
APPLE_ID={{APPLE_ID}}
APPLE_APP_SPECIFIC_PASSWORD={{APPLE_APP_SPECIFIC_PASSWORD}}
