# Monitoring and Analytics Configuration
# Copy this file to .env.monitoring and fill in your values

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1
SENTRY_SAMPLE_RATE=1.0
ENABLE_SENTRY_REPLAY=true

# LogRocket Configuration
LOGROCKET_APP_ID=your_logrocket_app_id_here

# PostHog Configuration (for Seer AI Debugger)
ENABLE_SEER=true
POSTHOG_SDK_KEY=your_posthog_sdk_key_here

# Telemetry Configuration
ENABLE_TELEMETRY=true
ENABLE_ANALYTICS=true
TELEMETRY_PRIVACY_MODE=false
LOG_LEVEL=info

# Google Cloud Monitoring (if using)
# GCP_PROJECT_ID=your-gcp-project-id
# GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Slack Notifications (optional)
# SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
