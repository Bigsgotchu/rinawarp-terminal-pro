{
  "name": "rinawarp-terminal",
  "version": "1.0.19",
  "description": "RinaWarp Terminal - An advanced commercial terminal emulator with AI assistance, themes, and enterprise features",
  "main": "src/main.cjs",
  "build": {
    "productName": "RinaWarp Terminal",
    "appId": "com.rinawarp.terminal",
    "copyright": "Copyright Â© 2025 Rinawarp Technologies, LLC",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "files": [
      "src/**/*",
      "public/**/*",
      "styles/**/*",
      "!src/tests/**/*",
      "!tests/**/*",
      "!**/*.test.js",
      "!dist",
      "!build",
      "!*.md",
      "!.git*",
      "!.env*",
      "!test-*.cjs"
    ],
    "extraResources": [
      {
        "from": "public",
        "to": "public",
        "filter": [
          "**/*"
        ]
      }
    ],
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "nodeGypRebuild": false,
    "forceCodeSigning": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": "x64"
        }
      ],
      "icon": "assets/icon.icns",
      "category": "public.app-category.developer-tools",
      "identity": null,
      "gatekeeperAssess": false,
      "hardenedRuntime": false,
      "entitlements": null,
      "entitlementsInherit": null,
      "notarize": false,
      "signIgnore": "(.*)"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "category": "Development"
    }
  },
  "author": {
    "name": "Rinawarp Technologies, LLC",
    "email": "rinawarptechnologies25@gmail.com",
    "url": "https://rinawarptech.com"
  },
  "homepage": "https://rinawarptech.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rinawarp-Terminal/rinawarp-terminal.git"
  },
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "electron src/main.cjs",
    "discord-bot": "node discord-bot.js",
    "server": "node server.js",
    "server-dev": "nodemon server.js",
    "test": "jest",
    "test:quick": "jest --maxWorkers=2 --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.cjs",
    "test:core": "node test-core-functionality.cjs",
    "copy-xterm": "node scripts/copy-xterm-assets.cjs",
    "copy-assets": "node scripts/copy-assets.cjs",
    "dev": "electron src/main.cjs --dev",
    "lint:fix": "eslint . --ext .js,.ts,.cjs --fix",
    "lint:rename-github": "pwsh ./scripts/dev-tools.ps1 rename-github",
    "lint:full-clean": "pwsh ./scripts/dev-tools.ps1 full-clean",
    "lint": "eslint .",
    "lint:check": "eslint .",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "qa": "npm run lint:check && npm run format:check && npm run test",
    "qa:fix": "npm run lint && npm run format && npm run test",
    "prebuild": "npm run copy-assets && npm run test:core",
    "build": "npm run prebuild && electron-builder",
    "build:web": "npm run copy-assets && echo 'Web build complete'",
    "build:webpack": "node build-system.cjs build",
    "build:webpack:watch": "node build-system.cjs watch",
    "build:webpack:serve": "node build-system.cjs serve",
    "build:win": "node scripts/build-windows.cjs",
    "build:mac": "node scripts/build-mac.cjs",
    "build:linux": "node scripts/build-linux.cjs",
    "build:all": "npm run prebuild && electron-builder --win --mac --linux",
    "build:quick": "electron-builder --publish=never",
    "build:production": "npm run prebuild && electron-builder --publish=onTagOrDraft",
    "build:css": "npx postcss styles/tailwind.css -o dist/tailwind.css",
    "build:css:watch": "npx postcss styles/tailwind.css -o dist/tailwind.css --watch",
    "dev:css": "concurrently \"npm run build:css:watch\" \"npm run dev\"",
    "test:railway": "node test-railway.js",
    "test:deployment": "node test-deployment.cjs",
    "deploy:test": "RAILWAY_URL=$RAILWAY_URL node test-deployment.js",
    "deploy:verify": "node test-deployment.js",
    "railway:health": "curl -f $RAILWAY_URL/health || exit 1",
    "postdeploy": "npm run deploy:verify",
    "release": "node scripts/release.cjs patch",
    "release:patch": "node scripts/release.cjs patch",
    "release:minor": "node scripts/release.cjs minor",
    "release:major": "node scripts/release.cjs major",
    "release:prerelease": "node scripts/release.cjs prerelease",
    "release:dry-run": "node scripts/release.cjs patch --dry-run",
    "release:status": "node scripts/release.cjs status",
    "release:changelog": "node scripts/release.cjs changelog",
    "fix:unused-vars": "node scripts/fix-unused-vars.cjs",
    "fix:all": "npm run fix:unused-vars && npm run lint && npm run format",
    "validate:structure": "node scripts/validate-structure.js",
    "validate:all": "npm run validate:structure && npm run lint:check && npm run test",
    "modernize:deps": "node cleanup-deps.js",
    "modernize:workflows": "node scripts/upgrade-workflows-to-v4.cjs",
    "modernize:packages": "node scripts/auto-replace-deprecated.js",
    "modernize:all": "npm run modernize:deps && npm run modernize:workflows && npm run modernize:packages",
    "scan:deprecated": "node scan-deprecated.cjs",
    "security:check": "npm audit --audit-level=moderate && npm run scan:deprecated",
    "monitor:workflows": "node scripts/monitor-workflows.cjs",
    "monitor:watch": "watch -n 300 'npm run monitor:workflows' -t",
    "check:workflows": "node scripts/check-workflows.cjs",
    "monitor:live": "node scripts/monitor-live.cjs",
    "prepare": "husky",
    "setup:monitoring": "node scripts/setup-monitoring.js",
    "setup:monitoring:manual": "node scripts/setup-monitoring.js manual",
    "test:monitoring": "node scripts/setup-monitoring.js test",
    "audit:urls": "node scripts/url-audit.js",
    "fix:urls": "node scripts/fix-urls.js"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.5.1",
    "glob": "^11.0.3",
    "graphql": "^15.8.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.1.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "recast": "^0.23.9",
    "stream-browserify": "^3.0.0",
    "stripe": "^18.3.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-env": "7.28.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@google-cloud/logging": "11.2.0",
    "@google-cloud/monitoring": "5.3.0",
    "@jest/globals": "30.0.4",
    "@mui/material": "7.2.0",
    "@mui/material-pigment-css": "^7.2.0",
    "@npmcli/fs": "4.0.0",
    "@react-native-async-storage/async-storage": "1.24.0",
    "@tailwindcss/postcss": "4.1.11",
    "@xterm/addon-fit": "0.10.0",
    "@xterm/addon-web-links": "0.11.0",
    "@xterm/xterm": "5.5.0",
    "archiver": "7.0.1",
    "autoprefixer": "10.4.21",
    "axios": "1.10.0",
    "babel-jest": "30.0.4",
    "babel-loader": "10.0.0",
    "bcryptjs": "3.0.2",
    "buffer": "6.0.3",
    "concurrently": "9.2.0",
    "cors": "2.8.5",
    "crypto-browserify": "3.12.1",
    "css-loader": "7.1.2",
    "discord.js": "14.21.0",
    "dmg-license": "1.0.11",
    "dotenv": "17.2.0",
    "electron": "37.2.3",
    "electron-builder": "26.0.12",
    "eslint": "9.31.0",
    "express": "4.21.2",
    "express-graphql": "0.12.0",
    "express-rate-limit": "7.5.1",
    "express-validator": "7.2.1",
    "firebase": "11.10.0",
    "graphql": "15.10.1",
    "helmet": "8.1.0",
    "html-loader": "5.1.0",
    "http-proxy-middleware": "3.0.5",
    "husky": "9.1.7",
    "jest": "30.0.4",
    "jest-circus": "30.0.4",
    "jest-environment-jsdom": "30.0.4",
    "jest-fetch-mock": "3.0.3",
    "joi": "17.13.3",
    "jsonwebtoken": "9.0.2",
    "lightningcss": "1.30.1",
    "lint-staged": "16.1.2",
    "morgan": "1.5.3",
    "next": "15.4.1",
    "node-cron": "4.2.1",
    "node-fetch": "3.3.2",
    "nodemailer": "7.0.5",
    "nodemon": "3.1.10",
    "openai": "5.10.1",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "postcss": "8.5.6",
    "postcss-cli": "11.0.1",
    "postcss-loader": "8.1.1",
    "prettier": "3.6.2",
    "process": "0.11.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "recast": "0.23.11",
    "recharts": "2.15.4",
    "standard-version": "9.5.0",
    "stream-browserify": "3.0.0",
    "stripe": "18.3.0",
    "style-loader": "4.0.0",
    "tailwindcss": "3.4.17",
    "util": "0.12.5",
    "vm2": "3.9.19",
    "webpack": "5.100.2",
    "webpack-cli": "6.0.1",
    "ws": "8.18.3"
  },
  "overrides": {
    "rimraf": "^4.4.1",
    "glob": "^11.0.3",
    "@npmcli/move-file": "@npmcli/fs"
  },
  "lint-staged": {
    "*.{js,cjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
