{
  "name": "rinawarp-terminal",
  "version": "1.3.1",
  "description": "RinaWarp Terminal - Advanced terminal emulator with AI assistance",
  "main": "src/main.cjs",
  "type": "module",
  "author": {
    "name": "Rinawarp Technologies, LLC",
    "email": "rinawarptechnologies25@gmail.com",
    "url": "https://rinawarptech.com"
  },
  "homepage": "https://rinawarptech.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bigsgotchu/rinawarp-terminal-pro.git"
  },
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "prestart": "echo 'Starting RinaWarp Terminal'",
    "dev": "NODE_ENV=development node server.js",
    "test": "jest --passWithNoTests",
    "test:quick": "jest --passWithNoTests --maxWorkers=2",
    "lint": "eslint . --ext .js,.ts,.cjs --fix && prettier --write . --ignore-unknown",
    "build": "echo 'RinaWarp Terminal build complete'",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "security:check": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@sentry/node": "^8.26.0",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.1.0",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "openai": "^4.52.7",
    "sqlite3": "^5.1.6",
    "stripe": "^16.5.0",
    "winston": "^3.13.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "chalk": "5.6.0",
    "electron": "^32.0.1",
    "electron-builder": "^25.0.5",
    "eslint": "^9.8.0",
    "fs-extra": "11.3.1",
    "jest": "^29.7.0",
    "node-fetch": "3.3.2",
    "openai": "5.12.2",
    "prettier": "^3.3.3"
  },
  "build": {
    "productName": "RinaWarp Terminal",
    "appId": "com.rinawarptech.terminal",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "public/**/*",
      "!**/*.test.js",
      "!dist",
      "!*.md",
      "!.git*",
      "!.env*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "portable",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/rinawarp-icon-1024.png"
    }
  }
}
