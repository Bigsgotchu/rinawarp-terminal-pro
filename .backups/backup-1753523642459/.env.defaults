# Default Configuration Values
# These values are used as fallbacks when environment-specific values are not provided

# Application Configuration
NODE_ENV=development
APP_VERSION=1.0.19
LOG_LEVEL=info
DEBUG=false

# Logging Configuration
LOG_TO_FILE=true
LOG_DIRECTORY=./logs
MAX_LOG_SIZE=10MB
MAX_LOG_FILES=5

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_CLOUD_SYNC=false
ENABLE_CRASH_REPORTING=true
ENABLE_AUTO_UPDATES=false

# Telemetry Configuration
ENABLE_TELEMETRY=true
TELEMETRY_PRIVACY_MODE=strict
TELEMETRY_BATCH_SIZE=10
TELEMETRY_FLUSH_INTERVAL=30000
SYSTEM_METRICS_INTERVAL=30000

# Analytics Configuration
ENABLE_ANALYTICS=false

# Error Sampling Rates
SENTRY_SAMPLE_RATE=1.0
SENTRY_TRACES_SAMPLE_RATE=0.5
APPINSIGHTS_SAMPLING_PERCENTAGE=100

# Base URLs
PRICING_URL=https://rinawarptech.com/pricing
